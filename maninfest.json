{
    "manifest_version": 2,
    "name": "My Picture Extension",
    "version": "1.0",
    "description": "Adds a picture to the current page on click",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}